{
  "ts-node": {
    "compilerOptions": {
      "module": "commonJS"
    }
  },
  "scripts": {
    "build": "mkdirp build && run-s build:schematron build:json build:snowpack",
    "build:json": "run-p build:json:sef:* && run-p build:json:cli:*",
    "build:json:cli:assertion-views": "run-s build:json:cli:assertion-views:*",
    "build:json:cli:assertion-views:poam": "ts-node -r tsconfig-paths/register src/cli/index.ts create-assertion-view ./public/assertion-views-poam.json ../validations/rules/poam.sch",
    "build:json:cli:assertion-views:sap": "ts-node -r tsconfig-paths/register src/cli/index.ts create-assertion-view ./public/assertion-views-sap.json ../validations/rules/sap.sch",
    "build:json:cli:assertion-views:sar": "ts-node -r tsconfig-paths/register src/cli/index.ts create-assertion-view ./public/assertion-views-sar.json ../validations/rules/sar.sch",
    "build:json:cli:assertion-views:ssp": "ts-node -r tsconfig-paths/register src/cli/index.ts create-assertion-view ./public/assertion-views-ssp.json ../validations/rules/ssp.sch",
    "build:json:cli:schematron": "run-s build:json:cli:schematron:*",
    "build:json:cli:schematron:poam": "ts-node -r tsconfig-paths/register src/cli/index.ts parse-schematron ../validations/rules/poam.sch ./public/poam.json",
    "build:json:cli:schematron:sap": "ts-node -r tsconfig-paths/register src/cli/index.ts parse-schematron ../validations/rules/sap.sch ./public/sap.json",
    "build:json:cli:schematron:sar": "ts-node -r tsconfig-paths/register src/cli/index.ts parse-schematron ../validations/rules/sar.sch ./public/sar.json",
    "build:json:cli:schematron:ssp": "ts-node -r tsconfig-paths/register src/cli/index.ts parse-schematron ../validations/rules/ssp.sch ./public/ssp.json",
    "build:json:cli:xspec-summaries": "run-s build:json:cli:xspec-summaries:*",
    "build:json:cli:xspec-summaries:poam": "ts-node -r tsconfig-paths/register src/cli/index.ts create-xspec-summaries ../validations/test/rules/poam.xspec ./public/xspec-scenarios-poam.json",
    "build:json:cli:xspec-summaries:sap": "ts-node -r tsconfig-paths/register src/cli/index.ts create-xspec-summaries ../validations/test/rules/sap.xspec ./public/xspec-scenarios-sap.json",
    "build:json:cli:xspec-summaries:sar": "ts-node -r tsconfig-paths/register src/cli/index.ts create-xspec-summaries ../validations/test/rules/sar.xspec ./public/xspec-scenarios-sar.json",
    "build:json:cli:xspec-summaries:ssp": "ts-node -r tsconfig-paths/register src/cli/index.ts create-xspec-summaries ../validations/test/rules/ssp.xspec ./public/xspec-scenarios-ssp.json",
    "build:json:sef:assertion-grouping": "xslt3 -xsl:../validations/rules/assertion-grouping.xsl -export:public/assertion-grouping.sef.json -nogo",
    "build:json:sef:ssp-json-to-xml": "xslt3 -xsl:../../vendor/oscal/xml/convert/oscal_complete_json-to-xml-converter.xsl -export:public/oscal_complete_json-to-xml-converter.sef.json -nogo",
    "build:json:sef:poam.sch": "xslt3 -xsl:build/poam.xsl -export:public/poam.sef.json -nogo",
    "build:json:sef:sap.sch": "xslt3 -xsl:build/sap.xsl -export:public/sap.sef.json -nogo",
    "build:json:sef:sar.sch": "xslt3 -xsl:build/sar.xsl -export:public/sar.sef.json -nogo",
    "build:json:sef:ssp.sch": "xslt3 -xsl:build/ssp.xsl -export:public/ssp.sef.json -nogo",
    "build:schematron": "run-s build:schematron:*",
    "build:schematron:poam": "run-s build:schematron:poam:*",
    "build:schematron:poam:stage-1": "xslt3 -s:../validations/rules/poam.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_dsdl_include.xsl -o:build/poam-stage1.sch allow-foreign=true diagnose=true",
    "build:schematron:poam:stage-2": "xslt3 -s:build/poam-stage1.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_abstract_expand.xsl -o:build/poam-stage2.sch allow-foreign=true diagnose=true",
    "build:schematron:poam:stage-3": "xslt3 -s:build/poam-stage2.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_svrl_for_xslt2.xsl -o:build/poam.xsl allow-foreign=true diagnose=true",
    "build:schematron:sap": "run-s build:schematron:sap:*",
    "build:schematron:sap:stage-1": "xslt3 -s:../validations/rules/sap.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_dsdl_include.xsl -o:build/sap-stage1.sch allow-foreign=true diagnose=true",
    "build:schematron:sap:stage-2": "xslt3 -s:build/sap-stage1.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_abstract_expand.xsl -o:build/sap-stage2.sch allow-foreign=true diagnose=true",
    "build:schematron:sap:stage-3": "xslt3 -s:build/sap-stage2.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_svrl_for_xslt2.xsl -o:build/sap.xsl allow-foreign=true diagnose=true",
    "build:schematron:sar": "run-s build:schematron:sar:*",
    "build:schematron:sar:stage-1": "xslt3 -s:../validations/rules/sar.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_dsdl_include.xsl -o:build/sar-stage1.sch allow-foreign=true diagnose=true",
    "build:schematron:sar:stage-2": "xslt3 -s:build/sar-stage1.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_abstract_expand.xsl -o:build/sar-stage2.sch allow-foreign=true diagnose=true",
    "build:schematron:sar:stage-3": "xslt3 -s:build/sar-stage2.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_svrl_for_xslt2.xsl -o:build/sar.xsl allow-foreign=true diagnose=true",
    "build:schematron:ssp": "run-s build:schematron:ssp:*",
    "build:schematron:ssp:stage-1": "xslt3 -s:../validations/rules/ssp.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_dsdl_include.xsl -o:build/ssp-stage1.sch allow-foreign=true diagnose=true",
    "build:schematron:ssp:stage-2": "xslt3 -s:build/ssp-stage1.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_abstract_expand.xsl -o:build/ssp-stage2.sch allow-foreign=true diagnose=true",
    "build:schematron:ssp:stage-3": "xslt3 -s:build/ssp-stage2.sch -xsl:../../vendor/schematron/trunk/schematron/code/iso_svrl_for_xslt2.xsl -o:build/ssp.xsl allow-foreign=true diagnose=true",
    "build:schematron:summary": "xslt3 -s:../validations/rules/rules.xsl -xsl:../validations/rules/rules.xsl -o:public/rules.html",
    "build:snowpack": "snowpack build",
    "cli": "ts-node -r tsconfig-paths/register src/cli/index.ts",
    "clean": "rm -rf build coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "postinstall": "patch-package",
    "start": "snowpack dev",
    "test": "jest",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.3.3",
    "classnames": "^2.3.1",
    "commander": "^2.20.3",
    "fp-ts": "^2.11.1",
    "highlight.js": "^11.5.1",
    "io-ts": "^2.2.16",
    "node-fetch": "^3.2.4",
    "overmind": "^28.0.1",
    "overmind-react": "^29.0.1",
    "patch-package": "^6.4.7",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.15.1",
    "saxon-js": "^2.3.0",
    "uswds": "^2.12.0",
    "xml-formatter": "^2.6.1"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-modal": "^3.13.1",
    "@types/snowpack-env": "^2.3.3",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "jest-html-reporter": "^3.4.1",
    "jest-mock-extended": "^2.0.5",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass": "1.32.13",
    "snowpack": "^3.8.6",
    "snowpack-plugin-raw-file-loader": "^1.0.9",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "xslt3": "^2.3.0"
  }
}
