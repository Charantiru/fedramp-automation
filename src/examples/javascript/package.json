{
  "name": "fedramp-automation-example",
  "version": "1.0.0",
  "description": "Javascript example code for validating fedramp-automation Schematron rules",
  "scripts": {
    "build:poam": "xslt3 -xsl:../../validations/target/rules/poam.sch.xsl -export:dist/poam.sef.json -nogo",
    "build:sap": "xslt3 -xsl:../../validations/target/rules/sap.sch.xsl -export:dist/sap.sef.json -nogo",
    "build:sar": "xslt3 -xsl:../../validations/target/rules/sar.sch.xsl -export:dist/sar.sef.json -nogo",
    "build:ssp": "xslt3 -xsl:../../validations/target/rules/ssp.sch.xsl -export:dist/ssp.sef.json -nogo",
    "build": "npm run build:poam && npm run build:sap && npm run build:sar && npm run build:ssp",
    "build-and-test": "npm run build && npm run test",
    "test": "node ./src/validator.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSA/fedramp-automation.git"
  },
  "author": "",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/GSA/fedramp-automation/issues"
  },
  "homepage": "https://github.com/GSA/fedramp-automation#readme",
  "dependencies": {
    "saxon-js": "^2.4.0",
    "xslt3": "^2.4.0"
  }
}
